  "loginPage": {
    "url": "/practice-test-login/",
    "title": "Test Login"
  },
  "selectors": {
    "usernameInput": "#username",
    "passwordInput": "#password",
    "submitButton": "#submit",
    "errorMessage": "#error",
    "successMessage": ".post-title",
    "logoutLink": "a:contains('Log out')"
  },
  "validUser": {
    "username": "student",
    "password": "Password123",

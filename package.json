{
  "name": "cypressfeb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "cy:run": "npx cypress run",
    "cy:open": "npx cypress open",
    "prereport": "npm run clean:reports",
    "report": "npx mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/combined.json && npx marge cypress/reports/mochawesome/combined.json -f report -o cypress/reports/html",
    "clean:reports": "if exist cypress\\reports rmdir /s /q cypress\\reports",
    "test:report": "npm run clean:reports && npm run cy:run && npm run report",
    "test:day2": "npx cypress run --spec \"cypress/e2e/Day2/*.cy.js\"",
    "test:day3": "npx cypress run --spec \"cypress/e2e/Day3/*.cy.js\"",
    "test:day4": "npx cypress run --spec \"cypress/e2e/Day4/*.cy.js\"",
    "test:parallel": "npm-run-all --parallel test:day2 test:day3 test:day4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypresstrainingvs/cypressfeb.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/cypresstrainingvs/cypressfeb/issues"
  },
  "homepage": "https://github.com/cypresstrainingvs/cypressfeb#readme",
  "devDependencies": {
    "cypress": "^15.10.0",
    "cypress-file-upload": "^5.0.8",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.1",
    "mochawesome-report-generator": "^6.3.2",
    "npm-run-all": "^4.1.5"
  }
}
